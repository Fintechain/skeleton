{
  "serviceId": "complex-integration-test",
  "storage": {
    "rootPath": "./test-data/complex",
    "defaultEngine": "file",
    "engines": {
      "memory": {
        "maxSize": 1048576
      },
      "file": {
        "compression": true,
        "encryption": false
      }
    }
  },
  "plugins": {
    "autoLoad": true,
    "searchPaths": ["./plugins", "./test-plugins"],
    "blacklist": ["deprecated-plugin"]
  },
  "logging": {
    "level": "debug",
    "format": "json"
  }
} 